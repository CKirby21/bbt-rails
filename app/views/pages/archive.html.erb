<%= render "topbar" %>

<div class="content">
  <% @scored_event_map_event_scores.each do |scored_event, event_scores| %>
    <table>
      <tr>
        <th class="table-title" colspan="2">
          <%= "#{scored_event.name}, #{scored_event.date_of_event}" %>
        </th>
      </tr>
      <tr>
        <th>Team</th>
        <th>Score</th>
      </tr>
      <% event_scores.each do |event_score| %>
        <tr>
          <td><%= event_score.team.name %></td>
          <td><%= event_score.score %></td>
        </tr>
      <% end %>
    <table>
  <% end %>
</div>

